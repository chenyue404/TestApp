<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cy.testapp">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.AnimPopActivity" />
        <activity android:name=".Activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activity.DialogActivity"
            android:theme="@style/NoAnimActivity" />
        <activity android:name=".Activity.ScrollActivity" />
        <activity
            android:name=".Activity.DialogActivity2"
            android:theme="@style/style_profile_transparent" />
        <activity android:name=".Activity.FrescoActivity" />
    </application>

</manifest>